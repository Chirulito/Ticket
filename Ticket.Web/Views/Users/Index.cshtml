@using Ticket.Web.Models
@model IEnumerable<Ticket.Web.Models.User>



<h1>Lista de Usuarios</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Correo</th>
            <th>Contraseña</th>
            <th>Provincia</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Password</td>
                <td>@item.Province</td>
            </tr>
        }
    </tbody>
</table>
